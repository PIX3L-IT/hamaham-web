{
  "name": "hamaham-web",
  "version": "1.0.0",
  "description": "Sistema Hama-ham",
  "keywords": [
    "FeathersJS",
    "hama-ham",
    "Node",
    "Express"
  ],
  "license": "MIT",
  "author": "Pixel.IT",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "prettier": "npx prettier \"**/*.js\" --write",
    "mocha": "cross-env NODE_ENV=test mocha test/ --recursive --exit",
    "test": "npm run mocha",
    "bundle:client": "npm pack --pack-destination ./public"
  },
  "engines": {
    "node": ">= 22.12.0"
  },
  "feathers": {
    "language": "js",
    "packager": "npm",
    "database": "mongodb",
    "framework": "express",
    "transports": [
      "rest"
    ],
    "schema": false
  },
  "directories": {
    "test": "test"
  },
  "main": "config/index.js",
  "dependencies": {
    "@feathersjs/adapter-commons": "^5.0.33",
    "@feathersjs/authentication": "^5.0.33",
    "@feathersjs/authentication-client": "^5.0.33",
    "@feathersjs/configuration": "^5.0.33",
    "@feathersjs/errors": "^5.0.33",
    "@feathersjs/express": "^5.0.33",
    "@feathersjs/feathers": "^5.0.33",
    "@feathersjs/mongodb": "^5.0.33",
    "@feathersjs/schema": "^5.0.33",
    "@feathersjs/transport-commons": "^5.0.33",
    "@firebase/app": "^0.11.4",
    "@firebase/auth": "^1.10.0",
    "axios": "^1.8.4",
    "body-parser": "^2.2.0",
    "compression": "^1.8.0",
    "cookie": "^1.0.2",
    "dotenv": "^16.4.7",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-validator": "^7.2.1",
    "feathers-mongoose": "^8.5.1",
    "firebase-admin": "^13.2.0",
    "helmet": "^8.1.0",
    "method-override": "^3.0.0",
    "mongo-sanitize": "^1.1.0",
    "mongodb": "^6.14.2",
    "mongoose": "^8.12.1",
    "path": "^0.12.7",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@feathersjs/cli": "^5.0.33",
    "@feathersjs/rest-client": "^5.0.33",
    "axios": "^1.8.2",
    "cross-env": "^7.0.3",
    "mocha": "^11.1.0",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3"
  }
}
